    particlesJS("particles-js", {
        "particles": {
            "number": {
                "value": 50, // Decrease particle count for a more dynamic look
                "density": {
                    "enable": true,
                    "value_area": 800
                }
            },
            "color": {
                "value": ["#ffffff", "#30b7f0", "#30b7f0"] // Keeping your colors
            },
            "shape": {
                "type": "circle",
                "stroke": {
                    "width": 0,
                    "color": "#000000"
                }
            },
            "opacity": {
                "value": 0.6, // Slightly higher opacity for more visibility
                "random": true,
                "anim": {
                    "enable": false, // No opacity animation for stability
                }
            },
            "size": {
                "value": 5, // Increased size for larger raindrops
                "random": true, // Random size within a range for variation
                "anim": {
                    "enable": false, // Keep size stable, no changes during animation
                }
            },
            "line_linked": {
                "enable": false // Disable links between particles
            },
            "move": {
                "enable": true,
                "speed": 2, // Faster speed for a more raindrop-like effect
                "direction": "bottom", // Falling downwards like rain
                "random": false, // Less random for a straighter path
                "straight": true, // Ensure a straight line drop like raindrops
                "out_mode": "out", // Remove particles when they leave the screen
                "bounce": false,
                "attract": {
                    "enable": false
                }
            }
        },
        "interactivity": {
            "detect_on": "canvas",
            "events": {
                "onhover": {
                    "enable": false // Disable hover interaction
                },
                "onclick": {
                    "enable": false // Disable click interaction
                },
                "resize": true
            },
            "modes": {
                "grab": {
                    "distance": 400,
                    "line_linked": {
                        "opacity": 1
                    }
                }
            }
        },
        "retina_detect": true
    });
</script>

